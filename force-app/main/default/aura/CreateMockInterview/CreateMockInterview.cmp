<aura:component controller="CreateMockInterviewController">
    <aura:attribute name="Type" type="List"/>
    <aura:attribute name="Stack" type="List"/>
    <aura:attribute name="SelectedTypes" type="List"/>
    <aura:attribute name="TypeMap" type="Map"/>
    <aura:attribute name="SelectedStack" type="String"/>

    <aura:handler name="init" value="{!this}" action="{!c.doinit}"/>

    <lightning:select name="chooseStack" label="Choose Stack" aura:id="stack id" value="{!v.SelectedStack}" required="true">
        <aura:iteration items="{!v.Stack}" var="option">
            <option text="{!option}" value="{!option}"/>
        </aura:iteration>

    </lightning:select>

    <lightning:dualListbox name="typeSelection"
                           label="Select Types"
                           sourceLabel="Available"
                           selectedLabel="Selected"
                           fieldLevelHelp="Select your interview question types."
                           options="{!v.Type}"
                           onchange="{! c.holdSelected}"/>

                           



</aura:component>	
