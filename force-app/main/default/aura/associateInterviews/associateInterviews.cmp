<aura:component controller="associateInterviewController">
    <h1 style="text-align:center;"><b><u>Interviews</u></b></h1>
	<!--Attributes-->
	<aura:attribute name="meetings" type="Meeting__c" />
	<aura:attribute name="columns" type="List" />
	<!--Handlers-->
	<aura:handler name="init" value="{!this}" action="{!c.init}"/>
    <aura:handler event="c:AssociateName" action="{!c.handleEvent}"/>

	<!--Content-->
	<lightning:datatable
		data="{!v.meetings}"
		columns="{!v.columns}"
		keyField="id"
		hideCheckboxColumn="true" />
</aura:component>