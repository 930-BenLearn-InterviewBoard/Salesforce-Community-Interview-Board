<aura:component controller="completedInterviewController">
	<aura:attribute name="meetings" type="Meeting__c" />
	<aura:attribute name="columns" type="List" />
	<aura:handler name="init" value="{!this}" action="{!c.init}"/>
	<lightning:datatable
		data="{!v.meetings}"
		columns="{!v.columns}"
		keyField="id" />
</aura:component>	
